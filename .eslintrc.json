{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-use-before-define": ["error", { "functions": false, "classes": true }],
    "no-plusplus": 0,
    "react/no-unused-prop-types": 1,
    "jsx-a11y/no-static-element-interactions": 1,
    "react/forbid-prop-types": 1,
    "global-require": 0
  }
}

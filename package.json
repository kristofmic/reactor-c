{
  "name": "reactor-c",
  "version": "1.2.0",
  "description": "Base Node web service with React + Redux",
  "main": "server.js",
  "scripts": {
    "start": "node ./www",
    "start:dev": "CONFIG=dev ./node_modules/.bin/nodemon ./www",
    "test": "yarn run lint && yarn run test:client && yarn run test:server",
    "test:client": "BABEL_ENV=test TEST_ENV=client MOCHA_FILE=./test/results/test-results.client.xml ./node_modules/.bin/istanbul cover --dir=./test/coverage/client -x=*.spec.js --report=cobertura --report=lcov ./node_modules/.bin/_mocha -- --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,mocha-junit-reporter=- ./test/jsdom.conf.js ./test/istanbul.conf.js ./test/mocha.conf.js \"./client/**/*.spec.js\"",
    "test:server": "CONFIG=dev BABEL_ENV=test TEST_ENV=server MOCHA_FILE=./test/results/test-results.server.xml ./node_modules/.bin/istanbul cover --dir=./test/coverage/server -x=*.spec.js --report=cobertura --report=lcov ./node_modules/.bin/_mocha -- --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,mocha-junit-reporter=- ./test/conf.js ./test/istanbul.conf.js ./test/mocha.conf.js \"./controllers/**/*.spec.js\" \"./lib/**/*.spec.js\"",
    "build": "yarn run clean && yarn run build:dist && yarn run copy",
    "build:dev": "./node_modules/.bin/webpack --progress --colors",
    "build:dist": "./node_modules/.bin/webpack -p --progress --colors --bail",
    "build:watch": "./node_modules/.bin/webpack --progress --colors -w",
    "copy": "yarn run copy:images",
    "copy:images": "cp -r ./client/images/. ./public/img/",
    "clean": "yarn run clean:public",
    "clean:public": "rm -rf ./public",
    "lint": "yarn run lint:client && yarn run lint:server",
    "lint:client": "./node_modules/.bin/eslint client --ext=js --ext=jsx",
    "lint:server": "./node_modules/.bin/eslint controllers lib server.js",
    "validateConfig": "node ./bin/validate_env.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kristofmic/reactor-c.git"
  },
  "author": "C",
  "license": "Unlicense",
  "engines": {
    "node": ">= 6.9 < 7"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-eslint": "7.2.3",
    "babel-loader": "6.4.1",
    "babel-plugin-rewire": "1.1.0",
    "babel-template": "6.24.1",
    "babel-types": "6.24.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "css-loader": "0.28.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.11.1",
    "glob": "7.1.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "9.12.0",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "mocha-multi": "0.11.0",
    "node-sass": "3.13.1",
    "nodemon": "1.11.0",
    "postcss-loader": "1.3.3",
    "react-test-renderer": "15.5.4",
    "redux-devtools": "3.3.2",
    "sass-loader": "4.0.2",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "1.13.3",
    "webpack-bundle-analyzer": "2.4.0",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "axios": "0.16.1",
    "babel-core": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "1.6.2",
    "ejs": "2.5.6",
    "es6-promise": "4.1.0",
    "express": "4.15.2",
    "helmet": "3.5.0",
    "immutability-helper": "2.2.0",
    "lodash": "4.17.4",
    "morgan": "1.8.1",
    "node-env-conf": "2.0.0",
    "prop-types": "15.5.8",
    "qs": "6.4.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "3.0.5",
    "react-transition-group": "1.1.2",
    "redux": "3.6.0",
    "request": "2.81.0",
    "winston": "2.3.1"
  }
}

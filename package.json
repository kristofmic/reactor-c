{
  "name": "reactor-c",
  "version": "1.4.1",
  "description": "Base Node web service with React + Redux",
  "main": "server.js",
  "scripts": {
    "start": "node ./www",
    "start:dev": "CONFIG=dev ./node_modules/.bin/nodemon ./www",
    "test": "yarn run lint && yarn run test:client && yarn run test:server && yarn run validateConfig",
    "test:client": "BABEL_ENV=test TEST_ENV=client MOCHA_FILE=./test/results/test-results.client.xml ./node_modules/.bin/istanbul cover --dir=./test/coverage/client -x=*.spec.js --report=cobertura --report=lcov ./node_modules/.bin/_mocha -- --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,mocha-junit-reporter=- ./test/jsdom.conf.js ./test/istanbul.conf.js ./test/mocha.conf.js \"./client/**/*.spec.js\"",
    "test:server": "CONFIG=dev BABEL_ENV=test TEST_ENV=server MOCHA_FILE=./test/results/test-results.server.xml ./node_modules/.bin/istanbul cover --dir=./test/coverage/server -x=*.spec.js --report=cobertura --report=lcov ./node_modules/.bin/_mocha -- --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,mocha-junit-reporter=- ./test/conf.js ./test/istanbul.conf.js ./test/mocha.conf.js \"./controllers/**/*.spec.js\" \"./lib/**/*.spec.js\"",
    "build": "yarn run clean && yarn run build:dist && yarn run copy",
    "build:dev": "./node_modules/.bin/webpack --progress --colors",
    "build:dist": "./node_modules/.bin/webpack -p --progress --colors --bail",
    "build:watch": "./node_modules/.bin/webpack --progress --colors -w",
    "copy": "yarn run copy:images",
    "copy:images": "cp -r ./client/images/. ./public/img/",
    "clean": "yarn run clean:public",
    "clean:public": "rm -rf ./public",
    "lint": "yarn run stylelint && yarn run lint:client && yarn run lint:server",
    "lint:client": "./node_modules/.bin/eslint --fix client --ext=js --ext=jsx",
    "lint:server": "./node_modules/.bin/eslint --fix controllers lib server.js",
    "stylelint": "./node_modules/.bin/stylelint client/**/*.scss --fix --syntax scss",
    "validateConfig": "node ./bin/validate_env.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kristofmic/reactor-c.git"
  },
  "author": "C",
  "license": "Unlicense",
  "engines": {
    "node": ">= 6.10 < 7"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-rewire": "1.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-template": "6.25.0",
    "babel-types": "6.25.0",
    "chai": "4.1.0",
    "chai-as-promised": "7.1.1",
    "css-loader": "0.28.4",
    "enzyme": "2.9.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "glob": "7.1.2",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "11.1.0",
    "mocha": "3.5.0",
    "mocha-junit-reporter": "1.13.0",
    "mocha-multi": "0.11.0",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "postcss-loader": "2.0.6",
    "react-test-renderer": "15.6.1",
    "redux-devtools": "3.4.0",
    "sass-loader": "6.0.6",
    "sinon": "3.0.0",
    "sinon-chai": "2.12.0",
    "style-loader": "0.18.2",
    "stylelint": "8.0.0",
    "stylelint-config-standard": "17.0.0",
    "url-loader": "0.5.9",
    "webpack": "3.4.1",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-manifest-plugin": "1.2.1",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-core": "6.25.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "bluebird": "3.5.0",
    "body-parser": "1.17.2",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "1.7.0",
    "ejs": "2.5.7",
    "es6-promise": "4.1.1",
    "express": "4.15.3",
    "helmet": "3.8.1",
    "immutability-helper": "2.3.0",
    "lodash": "4.17.4",
    "morgan": "1.8.2",
    "node-env-conf": "3.0.0",
    "prop-types": "15.5.10",
    "qs": "6.5.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-transition-group": "2.2.0",
    "reactor-blocks": "https://github.com/kristofmic/reactor-blocks.git#1.0.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "request": "2.81.0",
    "winston": "2.3.1"
  }
}
